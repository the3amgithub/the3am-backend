{
  "kind": "collectionType",
  "collectionName": "event_details",
  "info": {
    "singularName": "event-detail",
    "pluralName": "event-details",
    "displayName": "EventDetail"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "name": {
      "type": "string"
    },
    "about": {
      "type": "text"
    },
    "date": {
      "type": "string"
    },
    "location": {
      "type": "string"
    },
    "img": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": false
    },
    "address": {
      "type": "json"
    },
    "event": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::event.event",
      "inversedBy": "event_details"
    }
  }
}
